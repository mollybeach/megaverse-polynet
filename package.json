{
    "name": "megaverse-universe",
    "version": "0.1.0",
    "description": "A React-based application that creates a new 'Megaverse' by minting an X-shaped pattern of POLYanets using a RESTful API.",
    "homepage": "https://mollybeach.github.io/megaverse-universe",
    "author": "mollybeach",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/mollybeach/megaverse-universe.git"
    },
    "scripts": {
        "dev": "next dev",
        "build": "NEXT_TELEMETRY_DISABLED=1 next build",
        "start": "next start",
        "lint": "next lint",
        "cloudflare": "npx @cloudflare/next-on-pages",
        "pages:build": "NEXT_TELEMETRY_DISABLED=1 next build && npx @cloudflare/next-on-pages --experimental-minify",
        "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static"
    },
    "license": "MIT",
    "dependencies": {
        "@radix-ui/react-icons": "^1.3.2",
        "@radix-ui/react-slot": "^1.1.0",
        "axios": "^1.7.7",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "lucide-react": "^0.460.0",
        "next": "14.0.4",
        "next-themes": "^0.4.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwind-merge": "^2.5.4",
        "null-loader": "^4.0.1"
    },
    "devDependencies": {
        "@cloudflare/next-on-pages": "^1.13.5",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "autoprefixer": "^10.4.20",
        "eslint": "^8",
        "eslint-config-next": "14.0.4",
        "gh-pages": "^6.2.0",
        "postcss": "^8",
        "tailwindcss": "^3.4.15",
        "typescript": "^5",
        "vercel": "^39.1.1",
        "wrangler": "^3.90.0"
    },
    "engines": {
        "node": "18.x",
        "pnpm": "8.7.1"
    },
    "packageManager": "pnpm@8.7.1"
}